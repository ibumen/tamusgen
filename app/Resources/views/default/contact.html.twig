            <div class='col-xs-12 col-sm-5 col-md-4 col-lg-3'>
                <form>
                    <div class="form-group">
                        <label for="sendername">Full Name</label>
                        <input type="text" class="form-control" id="sendername" name='sendername' placeholder="Full Name">
                    </div>
                    <div class="form-group">
                        <label for="email">Email address</label>
                        <input type="email" class="form-control" id="email" name='email' placeholder="Email">
                    </div>
                    <div class="form-group">
                        <label for="message">Message</label>
                        <textarea class="form-control" rows="3" id='msg' name='msg'></textarea>
                    </div>
                    <button type="submit" class="btn btn-default">Send</button>
                </form>
            </div>
            <div class='col-xs-12 col-sm-1 col-md-2 col-lg-3'>
            </div>
        </div>
    </div>
{% endblock %}